/* pages/booklist/booklist.wxss */
.booklist .search-container{
  position: relative;
}

.booklist .search-container > input {
  display: block;
  margin: 20rpx 70rpx;
  padding: 12rpx 20rpx;
  font-size: 32rpx;
  color: #828282;
  -webkit-box-shadow:0px 2px 20px 5px #f5f7f6;  
  -moz-box-shadow:0px 2px 20px 5px #f5f7f6;  
  box-shadow:0px 2px 20px 5px #f5f7f6;
}

.booklist .search-container > icon{
  position: absolute;
  right: 85rpx;
  top: 50%;
  margin-top: -20rpx;
  z-index: 2;
}

.booklist .books{
  display: none;
}

.booklist .shelves .sprite{
  background: url('data:image/png;base64,') no-repeat;
  height: 60rpx;
  background-size: 96% 100%;
  position: absolute;
  left: 2%;
  z-index: -1;
  width: 100%;
}