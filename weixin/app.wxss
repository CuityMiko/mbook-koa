/**app.wxss**/
@import "./static/font/iconfont.wxss";
.container {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  font-family: PingFangSC-Light,helvetica,'Heiti SC';
  font-size: 30rpx;
}

@keyframes fadeIn {
  0% { opacity: 0; }
  100% { opacity: 1; }
}

.fade-in {
  animation: fadeIn 1s both;
}
